<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/dbg"
        android:baselineAligned="false"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/eShd"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/shoulder" />

                    <TextView
                        android:id="@+id/shdReinForce"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/eShd"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="3dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:gravity="top"
                        android:text="+0"
                        android:textColor="#ffffff"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/shdMix"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="29dp"
                        android:layout_marginTop="40dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:text="융합"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />

                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/eShirt"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/shirt" />

                    <TextView
                        android:id="@+id/shirtReinForce"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/eShirt"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="3dp"
                        android:background="#80000000"
                        android:gravity="top"
                        android:text="+0"
                        android:fontFamily="@font/dnfmidium"
                        android:textColor="#ffffff"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/shirtMix"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="29dp"
                        android:layout_marginTop="40dp"
                        android:background="#80000000"
                        android:text="융합"
                        android:fontFamily="@font/dnfmidium"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />
                </RelativeLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/ePants"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/pants" />

                    <TextView
                        android:id="@+id/pantsReinForce"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/ePants"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="3dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:gravity="top"
                        android:text="+0"
                        android:textColor="#ffffff"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/pantsMix"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="29dp"
                        android:layout_marginTop="40dp"
                        android:background="#80000000"
                        android:text="융합"
                        android:fontFamily="@font/dnfmidium"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/eBelt"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/belt" />

                    <TextView
                        android:id="@+id/beltReinForce"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/eBelt"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="3dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:gravity="top"
                        android:text="+0"
                        android:textColor="#ffffff"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/beltMix"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="29dp"
                        android:layout_marginTop="40dp"
                        android:background="#80000000"
                        android:text="융합"
                        android:fontFamily="@font/dnfmidium"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />

                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/eShoe"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="1dp"
                        android:layout_marginBottom="1dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/shoes" />

                    <TextView
                        android:id="@+id/shoeReinForce"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/eShoe"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="3dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:gravity="top"
                        android:text="+0"
                        android:textColor="#ffffff"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/shoeMix"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="28.5dp"
                        android:layout_marginTop="39dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:text="융합"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="wrap_content"
                        android:layout_height="60dp"
                        android:layout_margin="1dp"
                        android:layout_weight="1"
                        android:visibility="invisible"
                        app:srcCompat="@drawable/shoes" />
                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_margin="1dp"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/charPicture"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:scaleType="centerCrop"
                    android:visibility="visible"
                    app:srcCompat="@drawable/loading" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="53dp"
                    android:gravity="bottom|center"
                    android:orientation="horizontal">
                <TextView
                    android:id="@+id/advName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginBottom="2dp"
                    android:fontFamily="@font/dnfbold"
                    android:text="advName"
                    android:textColor="#ffffff"
                    android:textSize="10sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="25dp"
                    android:gravity="bottom|center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/userName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginBottom="2dp"
                        android:fontFamily="@font/dnfbold"
                        android:text="NickName"
                        android:textColor="#ffffff"
                        android:textSize="20sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp"
                    android:gravity="bottom|center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/famepng"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        app:srcCompat="@drawable/fame" />

                    <TextView
                        android:id="@+id/fameNum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:fontFamily="@font/nexon"
                        android:text="0"
                        android:textColor="#ffffff"
                        android:textSize="12.5sp" />
                </LinearLayout>

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <RelativeLayout
