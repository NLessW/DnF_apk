                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="38dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:text="융합"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />
                    <TextView
                        android:id="@+id/neckSun"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="38dp"
                        android:background="#171717"
                        android:fontFamily="@font/dnfbold"
                        android:text=" 仙 "
                        android:textColor="#ff00"
                        android:textSize="12sp"
                        android:visibility="visible" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/eSub"
                        android:layout_width="110dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="1dp"
                        android:layout_marginBottom="1dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/sub" />

                    <TextView
                        android:id="@+id/subReinForce"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/eSub"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="3dp"
                        android:background="#80000000"
                        android:gravity="top"
                        android:text="+0"
                        android:fontFamily="@font/dnfmidium"
                        android:textColor="#ffffff"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/subMix"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="38dp"
                        android:background="#80000000"
                        android:fontFamily="@font/dnfmidium"
                        android:text="융합"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/subSun"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="38dp"
                        android:background="#171717"
                        android:fontFamily="@font/dnfbold"
                        android:text=" 仙 "
                        android:textColor="#ff00"
                        android:textSize="12sp"
                        android:visibility="visible" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="110dp"
                    android:layout_height="60dp"
                    android:layout_margin="1dp"
                    android:layout_weight="1">

                    <ImageView
                        android:id="@+id/eRing"
                        android:layout_width="110dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="1dp"
                        android:layout_marginBottom="1dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/ring" />

                    <TextView
                        android:id="@+id/ringReinForce"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/eRing"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="3dp"
                        android:background="#80000000"
                        android:gravity="top"
                        android:text="+0"
                        android:fontFamily="@font/dnfmidium"
                        android:textColor="#ffffff"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/ringMix"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="38dp"
                        android:background="#80000000"
                        android:text="융합"
                        android:fontFamily="@font/dnfmidium"
                        android:textColor="#ffffff"
                        android:textSize="13sp"
                        android:visibility="visible" />
                    <TextView
